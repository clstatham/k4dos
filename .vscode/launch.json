{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "custom",
            "name": "Attach to gdbserver",
            "initCommands": [
                "platform select remote-gdb-server"
            ],
            "targetCreateCommands": [
                "target create ${workspaceFolder}/target/iso_root/boot/k4dos"
            ],
            "processCreateCommands": [
                "gdb-remote 127.0.0.1:1234"
            ]
        },
        
    ]
}